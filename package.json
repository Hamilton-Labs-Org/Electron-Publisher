{
  "name": "electron-tutorial",
  "productName": "Hamilton Labs",
  "version": "1.1.5-alpha",
  "description": "Electron Publisher Tutorial",
  "main": "./src/main/main.mjs",
  "scripts": {
    "preview": "electron-vite preview --outDir=dist",
    "dev": "electron-vite dev --watch --outDir=dist -- --trace-warnings",
    "prebuild": "electron-vite build --outDir=dist -- --trace-warnings",
    "start": "electron-forge start",
    "test": "echo \"Error: no test specified\" && exit 1",
    "package": "electron-vite build --outDir=dist && electron-forge package",
    "build": "electron-forge package",
    "make": "electron-vite build --outDir=dist && electron-forge make",
    "publish": "electron-forge publish"
  },
  "keywords": [],
  "author": "Hamilton Labs",
  "license": "MIT",
  "type": "module",
  "devDependencies": {
    "@electron-forge/cli": "^7.4.0",
    "@electron-forge/maker-deb": "^7.4.0",
    "@electron-forge/maker-rpm": "^7.4.0",
    "@electron-forge/maker-squirrel": "^7.4.0",
    "@electron-forge/maker-zip": "^7.4.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.4.0",
    "@electron-forge/plugin-fuses": "^7.4.0",
    "@electron-forge/publisher-github": "^7.4.0",
    "electron": "^31.3.1",
    "electron-vite": "^2.3.0",
    "@electron-toolkit/preload": "^3.0.1",
    "@electron/asar": "^3.2.10"
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.1"
  }
